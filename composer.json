{
    "name": "codingwithrk/chuck-norris-jokes",
    "description": "Create random Chuck Norris jokes",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codingwithrk\\ChuckNorrisJokes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codingwithrk\\ChuckNorrisJokes\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "CodingwithRK",
            "email": "pappalarajkumar@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "orchestra/testbench": "^9.1"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codingwithrk\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases" : {
                "ChuckNorris": "Codingwithrk\\ChuckNorrisJokes\\Facades\\ChuckNorris"
            }
        }
    }
}
